{
    "builds": [
      {
        "src": "FOODZONE/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic",
            "outputDirectory": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "FOODZONE/wsgi.py"
      }
    ]
}