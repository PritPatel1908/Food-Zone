{
  "builds": [
    {
      "src": "FOODZONE/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles_build/static" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/"
    },
    {
      "src": "/(.*)",
      "dest": "FOODZONE/wsgi.py"
    }
  ]
}